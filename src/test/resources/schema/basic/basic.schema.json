{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://example.com/schema/json/common/basic/v1",
  "title": "Basic",
  "type": "object",
  "properties": {
    "foo": {
      "type": "string"
    },
    "foo_under": {
      "type": "string"
    },
    "price": {
      "type": "string",
      "format": "decimal"
    },
    "quantity": {
      "type": "number"
    },
    "temp": {
      "type": "integer",
      "minimum": 1,
      "maximum": 100
    },
    "test-object": {
      "type": "object",
      "properties": {
        "foo_too": {
          "type": "string"
        }
      }
    },
    "my-basic-ref": {
      "$ref": "/schema/json/common/basic-ref/v1"
    }
  },
  "required": ["foo", "my-basic-ref"]
}